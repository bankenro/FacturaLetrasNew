<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".Fragments.MenuFragment"
                                                   android:id="@+id/constraintLayout3">
    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
    >

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <ImageButton
                    android:src="@drawable/addfactura"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/addfactura" android:background="#00FFFFFF"
                    tools:visibility="gone"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/facturas"/>
            <ImageButton
                    android:src="@drawable/facturas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/facturas" android:background="#00FFFFFF"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/addfactura" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/letras"/>
            <ImageButton
                    android:src="@drawable/reportes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/reportes" android:background="#00FFFFFF"
                    tools:visibility="gone"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/letras" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/registro"/>
            <ImageButton
                    android:src="@drawable/registro"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/registro" android:background="#00FFFFFF"
                    tools:visibility="gone"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.502" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/reportes"
                    app:layout_constraintBottom_toTopOf="@+id/empreclie"/>
            <ImageButton
                    android:src="@drawable/letras"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/letras" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/facturas" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:background="#FFFFFF"
                    app:layout_constraintBottom_toTopOf="@+id/reportes"/>
            <ImageButton
                    android:src="@drawable/emprclie"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/empreclie"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:background="#FFFFFF"
                    tools:visibility="gone"
                    app:layout_constraintHorizontal_bias="0.502"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/registro"
                    app:layout_constraintBottom_toTopOf="@+id/actcod"/>
            <ImageButton
                    android:src="@drawable/actcod"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/actcod" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/empreclie"
                    app:layout_constraintBottom_toTopOf="@+id/salir" tools:visibility="gone"/>
            <ImageButton
                    android:src="@drawable/salir"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/salir" android:background="#00FFFFFF"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.502"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/actcod"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>